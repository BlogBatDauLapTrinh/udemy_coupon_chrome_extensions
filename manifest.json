{
  "manifest_version": 2,
  "name": "Auto enroll 100% sale-off Udemy Coupons",
  "description": "This extension save your money by enroll 100% sale off course in Udemy",
  "version": "0.1",
  "icons": {
    "192": "icons/icon192x192.png",
    "32": "icons/icon32x32.png",
    "16": "icons/icon16x16.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16x16.png",
    "default_popup": "popup.html",
    "default_title": "Blog BatDauLapTrinh"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "include_globs": [
        "*://*.udemy.*/*"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus",
    "activeTab",
    "tabs"
  ],
  "commands": {
    "show_all_coupons": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "show all available coupons in new tab"
    },
    "auto_enroll_all_coupons": {
      "suggested_key": {
        "default": "Alt+A"
      },
      "description": "auto to enroll all coupons"
    },
    "pause": {
      "suggested_key": {
        "default": "Alt+P"
      },
      "description": "pause auto"
    },
    "continue": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "continue auo"
    }
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' https://batdaulaptrinh.com; object-src 'self'"
}